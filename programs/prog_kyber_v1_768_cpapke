`ifdef DECRYPT

# Inputs  : CTXT = ( Poly_24 , Poly_25 , Poly_26 , Poly_20 ) ; SK = ( Poly_16 , Poly_17 , Poly_18 )
# Outputs : PTXT = ( Poly_15 )

load ( poly = 24 , "data/kyber_v1_768/ct_1.npy" )
print ( poly = 24 )
load ( poly = 25 , "data/kyber_v1_768/ct_2.npy" )
print ( poly = 25 )
load ( poly = 26 , "data/kyber_v1_768/ct_3.npy" )
print ( poly = 26 )
load ( poly = 20 , "data/kyber_v1_768/ct_4.npy" )
print ( poly = 20 )
load ( poly = 16 , "data/kyber_v1_768/sk_1.npy" )
print ( poly = 16 )
load ( poly = 17 , "data/kyber_v1_768/sk_2.npy" )
print ( poly = 17 )
load ( poly = 18 , "data/kyber_v1_768/sk_3.npy" )
print ( poly = 18 )

# NTT (U)
mult_psi ( poly = 24 )
transform ( mode = DIF_NTT , poly_dst = 0 , poly_src = 24 )
mult_psi ( poly = 25 )
transform ( mode = DIF_NTT , poly_dst = 1 , poly_src = 25 )
mult_psi ( poly = 26 )
transform ( mode = DIF_NTT , poly_dst = 2 , poly_src = 26 )

# S * NTT (U)
poly_op ( op = MUL , poly_dst = 0 , poly_src = 16 )
poly_op ( op = MUL , poly_dst = 1 , poly_src = 17 )
poly_op ( op = MUL , poly_dst = 2 , poly_src = 18 )
init ( poly = 21 )
poly_op ( op = ADD , poly_dst = 21 , poly_src = 0 )
poly_op ( op = ADD , poly_dst = 21 , poly_src = 1 )
poly_op ( op = ADD , poly_dst = 21 , poly_src = 2 )

# INTT (S * NTT (U))
transform ( mode = DIT_INTT , poly_dst = 15 , poly_src = 21 )
mult_psi_inv ( poly = 15 )

# v - INTT (S * NTT (U))
poly_op ( op = SUB , poly_dst = 15 , poly_src = 20 )

print ( poly = 15 )
save ( poly = 15 , "data/kyber_v1_768/pt_out.npy" )
#encode_print ( poly = 15 , encoding = BINARY_0RED )
encode_compare ( "data/kyber_v1_768/pt_in.npy" , "data/kyber_v1_768/pt_out.npy" , encoding = BINARY_0RED )

`endif

