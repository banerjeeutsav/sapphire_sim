print ( poly = 29 )
load ( poly = 30 , "data/kyber_v1_768/pk_4.npy" )
print ( r0 )
random ( r1 )
print ( r1 )
random ( poly = 15 , encoding = BINARY_0RED, "data/kyber_v1_768/pt_in.npy" )
print ( poly = 15 )

# Sample (R)
bin_sample ( prng = SHAKE-256 , seed = r1 , c0 = 0 , c1 = 0 , k = 4 , poly = 4 )
#print ( poly = 4 )
bin_sample ( prng = SHAKE-256 , seed = r1 , c0 = 0 , c1 = 1 , k = 4 , poly = 5 )
