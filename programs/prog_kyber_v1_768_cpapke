bin_sample ( prng = SHAKE-256 , seed = r1 , c0 = 0 , c1 = 6 , k = 4 , poly = 12 )
#print ( poly = 12 )

# INTT (AT * NTT (R))
transform ( mode = DIT_INTT , poly_dst = 8 , poly_src = 20 )
mult_psi_inv ( poly = 8 )
