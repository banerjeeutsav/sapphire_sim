mult_psi ( poly = 5 )
transform ( mode = DIF_NTT , poly_dst = 17 , poly_src = 5 )
mult_psi ( poly = 6 )
transform ( mode = DIF_NTT , poly_dst = 18 , poly_src = 6 )

